{
  "name": "xinfox/xinfox-skeleton",
  "description": "XinFox basic application skeleton",
  "type": "project",
  "authors": [
    {
      "name": "令狐冲",
      "email": "nicolaslei@163.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "topthink/framework": "^6.0",
    "xinfox/think-annotation": "dev-main",
    "xinfox/jwt-auth": "dev-main",
    "xinfox/sms": "^1.0",
    "gregwar/captcha": "^1.1",
    "psr/simple-cache": "^1.0"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../think-annotation",
      "options": {
        "symlink": false
      }
    },
    {
      "type": "path",
      "url": "../xinfox-auth",
      "options": {
        "symlink": false
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "XinFox\\": "app"
    }
  }
}
